#include <iostream>

#define ROW 3
#define COL 3

int main() {
    int arr[ROW][COL];

    // Taking user input
    std::cout << "Enter the elements of the array: \n";
    for(int i = 0; i < ROW; i++) {
        for(int j = 0; j < COL; j++) {
            std::cout << "Enter element at position [" << i << "][" << j << "]: ";
            std::cin >> arr[i][j];
        }
    }

    // Displaying the array
    std::cout << "\nDisplaying the array: \n";
    for(int i = 0; i < ROW; i++) {
        for(int j = 0; j < COL; j++) {
            std::cout << arr[i][j] << " ";
        }
        std::cout << std::endl;
    }

    return 0;
}
